{
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration"
  },
  "Routes": [
    {
      "SwaggerKey": "contentrecsys",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "contentrecsys",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "SwaggerKey": "identity-api",
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/v1/identity/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "PATCH", "DELETE" ]
    },

    {
      "SwaggerKey": "catalog-api",
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "bookcatalog.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/v1/catalog/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "PATCH", "DELETE" ]
    },

    {
      "SwaggerKey": "basket-api",
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "basket.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/v1/basket/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "DELETE" ]
    },

    {
      "SwaggerKey": "ordering-api",
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "ordering.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/v1/ordering/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "DELETE", "PATCH" ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "identity-api",
      "Config": [
        {
          "Name": "Identity API",
          "Version": "v1",
          "Url": "http://identity.api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "catalog-api",
      "Config": [
        {
          "Name": "Catalog API",
          "Version": "v1",
          "Url": "http://bookcatalog.api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "basket-api",
      "Config": [
        {
          "Name": "Basket API",
          "Version": "v1",
          "Url": "http://basket.api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "ordering-api",
      "Config": [
        {
          "Name": "Ordering API",
          "Version": "v1",
          "Url": "http://ordering.api:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}